ldenContext.Provider value={{orientation, size}}>\n          {children}\n        </goldenContext.Provider>\n      </div>\n    </div>\n  )\n}\n\nexport function Main({\n  children,\n  style,\n  attr,\n}) {\n  return (\n    <main\n      style={{\n        flex: 1,\n        ...style,\n      }}\n      {...attr}\n    >\n      {children}\n    </main>\n  );\n}\n\nexport function Aside({\n  children,\n  style,\n  attr,\n}) {\n  return (\n    <aside\n      style={{\n        flex: phi,\n        ...style\n      }}\n      {...attr}\n    >\n      {children}\n    </aside>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"d17d06efbaf0f87c19216ccde2196f4d","cacheData":{"env":{}}}